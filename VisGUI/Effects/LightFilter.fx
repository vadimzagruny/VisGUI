#include"Header.hlsli"



//--------------------------------------------------------------------------------------
// Pixel Shader
//--------------------------------------------------------------------------------------
void PS( in  float4 dif : COLOR,
         out float4 ocolor  : COLOR )
{
	ocolor =  float4(dif);
}


//--------------------------------------------------------------------------------------
// Default Technique
//--------------------------------------------------------------------------------------
technique tec0
{
    pass p0
    {
		ZEnable          = TRUE;
		ZWriteEnable     = TRUE;

		PixelShader  = compile ps_3_0 PS();

    }
}
